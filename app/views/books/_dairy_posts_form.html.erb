<div class="my-3">
  <%= from_with url: user_dairy_posts_path(user), method: :get, class: "row justify-content-center", local: false do |f| %>
    <div class="col-auto">
      <%= f.date_field :created_at, class="form-controll" %>
    </div>
    <div class="col-auto">
      <%= f.submit '検索', class: "btn btn-primary" %>
    </div>
  <% end %>
</div>

<div class="dairy_posts_table">
</div>